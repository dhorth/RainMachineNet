<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>David Horth</Authors>
    <Company>IRC, LLC</Company>
    <Description>dot net library for RainMachine REST library</Description>
    <Copyright>2021</Copyright>
    <PackageTags>RainMachine</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
    <Version>0.0.1</Version>
    <PackageDescription>dotNet library for control and configuration of RainMachine devices</PackageDescription>
    <RepositoryUrl>https://github.com/dhorth/RainMachineNet</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="RestSharp" Version="106.11.7" />
    <PackageReference Include="Serilog" Version="2.10.0" />
  </ItemGroup>

</Project>
